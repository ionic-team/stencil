{
  "name": "@stencil/core",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "bin": {
    "stencil": "./bin/stencil"
  },
  "engines": {
    "node": ">=6.4.0"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run transpile && npm run build.compiler && npm run build.web && npm run build.angular && npm run build.ssr",
    "build.compiler": "npm run transpile.compiler && node dist/scripts/build-compiler.js",
    "build.web": "node dist/scripts/build-web.js",
    "build.angular": "npm run copy.angular && node dist/scripts/build-angular.js",
    "build.ssr": "tsc -p src/bindings/server-renderer/tsconfig.json && node dist/scripts/build-ssr.js",
    "clean": "rimraf dist/",
    "copy.angular": "node dist/scripts/copy-angular.js",
    "dev": "(npm run transpile && node dist/scripts/dev-watch.js) & npm run transpile.watch",
    "start": "npm install && npm run build",
    "test": "(jest --no-cache || true)",
    "test.watch": "jest --watch",
    "transpile": "tsc -p scripts/tsconfig.json & tsc -p src/bindings/web/tsconfig.json & tsc -p src/bindings/angular/tsconfig.json & tsc -p src/bindings/server-renderer/tsconfig.json",
    "transpile.compiler": "tsc -p src/compiler/tsconfig.json",
    "transpile.watch": "tsc -p scripts/tsconfig.json -w & tsc -p src/bindings/web/tsconfig.json -w & tsc -p src/bindings/angular/tsconfig.json -w & tsc -p src/bindings/server-renderer/tsconfig.json -w"
  },
  "dependencies": {
    "ionicons": "3.0.0"
  },
  "devDependencies": {
    "@types/babel-core": "^6.7.14",
    "@types/css": "0.0.30",
    "@types/fs-extra": "^2.0.0",
    "@types/jest": "18.1.1",
    "@types/jsdom": "^11.0.0",
    "@types/ncp": "^2.0.0",
    "@types/node": "7.0.5",
    "@types/node-fetch": "^1.6.7",
    "@types/swiper": "^3.4.2",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-plugin-transform-define": "^1.2.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.23.0",
    "babel-plugin-transform-es2015-classes": "^6.23.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.23.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.22.0",
    "babel-plugin-transform-es2015-template-literals": "^6.22.0",
    "fs-extra": "^2.1.2",
    "google-closure-compiler": "20170218.0.0",
    "jest": "19.0.2",
    "jsdom": "^11.0.0",
    "knuth-shuffle": "1.0.1",
    "ncp": "^2.0.0",
    "node-fetch": "^1.7.0",
    "node-sass": "^4.5.0",
    "parse5": "^3.0.2",
    "rimraf": "2.6.1",
    "rollup": "0.41.4",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "swiper": "^3.4.2",
    "tslint": "^5.1.0",
    "tslint-ionic-rules": "0.0.8",
    "typescript": "2.4.0",
    "xmlhttprequest": "^1.8.0"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "<rootDir>/scripts/jest.preprocessor.js"
    },
    "testRegex": "/src/.*\\.spec\\.(ts|js)$"
  }
}
