{
  "name": "@stencil/core",
  "version": "1.8.2-1",
  "license": "MIT",
  "main": "internal/stencil.core.js",
  "types": "internal/stencil.core.d.ts",
  "bin": {
    "stencil": "bin/stencil"
  },
  "files": [
    "bin/",
    "cli/",
    "compiler/",
    "dev-server/",
    "internal/",
    "mock-doc/",
    "screenshot/",
    "sys/",
    "testing/"
  ],
  "scripts": {
    "start": "npm run watch",
    "build": "node scripts && npm run tsc && npm run rollup.prod.ci",
    "build.prod": "node scripts && npm run tsc.scripts && npm run tsc && npm run rollup.prod",
    "tsc.scripts": "tsc -p scripts/tsconfig.json",
    "watch": "node scripts && npm run tsc && concurrently \"npm run rollup.watch\" \"npm run tsc.watch\"",
    "release": "node scripts --release --publish",
    "release.prepare": "node scripts --release --prepare",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "tslint --project tsconfig.json",
    "tsc": "tsc --incremental",
    "tsc.watch": "tsc --incremental --watch",
    "rollup": "rollup --config",
    "rollup.prod": "rollup --config --config-prod",
    "rollup.prod.ci": "rollup --config --config-prod --config-ci",
    "rollup.watch": "rollup --watch --config",
    "license": "node scripts --license",
    "test": "jest",
    "test.dist": "node scripts --validate-build",
    "test.end-to-end": "cd test/end-to-end && npm test",
    "test.jest": "jest",
    "test.karma": "cd test/karma && npm run karma",
    "test.karma.prod": "cd test/karma && npm run karma.prod",
    "test.testing": "node scripts/test/validate-testing.js",
    "test.sys.node": "echo test.sys.node",
    "test.prod": "npm run test.dist && npm run test.end-to-end && npm run test.jest && npm run test.karma && npm run test.sys.node && npm run test.testing",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@types/puppeteer": "^2.0.0",
    "concurrently": "^5.0.0",
    "execa": "^3.4.0",
    "fast-deep-equal": "^3.1.1",
    "inquirer": "^7.0.0",
    "open": "^7.0.0",
    "pixelmatch": "^5.1.0",
    "puppeteer": "^2.0.0",
    "tslint-ionic-rules": "0.0.21",
    "typescript": "3.7.2"
  },
  "devDependencies": {
    "@types/autoprefixer": "^9.1.1",
    "@types/cssnano": "^4.0.0",
    "@types/exit": "^0.1.30",
    "@types/fs-extra": "^8.0.0",
    "@types/glob": "^7.1.1",
    "@types/graceful-fs": "^4.1.2",
    "@types/inquirer": "6.5.0",
    "@types/is-glob": "^4.0.0",
    "@types/jest": "^24.0.20",
    "@types/listr": "^0.14.2",
    "@types/mime-types": "^2.1.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.7.1",
    "@types/node-fetch": "^2.3.5",
    "@types/open": "^6.1.0",
    "@types/parse5": "^5.0.0",
    "@types/pixelmatch": "^5.0.0",
    "@types/pngjs": "^3.3.2",
    "@types/prompts": "2.0.1",
    "@types/rollup-plugin-json": "^3.0.2",
    "@types/semver": "^6.0.1",
    "@types/webpack": "^4.39.1",
    "@types/ws": "^6.0.1",
    "ansi-colors": "4.1.1",
    "autoprefixer": "9.7.3",
    "conventional-changelog-cli": "^2.0.12",
    "core-js-builder": "^3.2.0",
    "css-what": "3.2.1",
    "cssnano": "4.1.10",
    "dts-bundle-generator": "^3.2.0",
    "exit": "^0.1.2",
    "fs-extra": "^8.1.0",
    "glob": "7.1.6",
    "graceful-fs": "^4.2.1",
    "is-glob": "4.0.1",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "jest-environment-node": "^24.0.0",
    "listr": "0.14.3",
    "magic-string": "^0.25.2",
    "mime-db": "1.42.0",
    "minimatch": "3.0.4",
    "mkdirp": "^0.5.1",
    "node-fetch": "2.6.0",
    "open-in-editor": "2.2.0",
    "parse5": "5.1.1",
    "path-browserify": "^1.0.0",
    "postcss": "7.0.23",
    "prompts": "2.3.0",
    "rollup": "1.27.8",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "semver": "6.3.0",
    "stream-browserify": "^2.0.2",
    "terser": "4.4.2",
    "tslib": "^1.10.0",
    "tslint": "^5.16.0",
    "util": "^0.12.0",
    "webpack": "^4.41.2",
    "ws": "7.1.2"
  },
  "lazyDependencies": {
    "@types/jest": "~24.0.20",
    "@types/puppeteer": "^1.19.0",
    "jest": "~24.9.0",
    "jest-cli": "~24.9.0",
    "pixelmatch": "4.0.2",
    "puppeteer": "^1.19.0",
    "puppeteer-core": "^1.19.0",
    "workbox-build": "~4.3.1"
  },
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil.git"
  },
  "author": "Ionic Team",
  "homepage": "https://stenciljs.com/",
  "description": "A Compiler for Web Components and Progressive Web Apps",
  "keywords": [
    "web components",
    "components",
    "stencil",
    "ionic",
    "webapp",
    "custom elements",
    "pwa",
    "progressive web app"
  ],
  "jest": {
    "preset": "./testing/jest-preset.js",
    "moduleNameMapper": {
      "@app-data": "<rootDir>/internal/app-data/index.js",
      "@compiler": "<rootDir>/compiler/index.js",
      "@mock-doc": "<rootDir>/mock-doc/index.js",
      "@platform": "<rootDir>/internal/testing/index.js",
      "@runtime": "<rootDir>/internal/runtime/index.js",
      "@testing": "<rootDir>/testing/index.js",
      "@utils": "<rootDir>/src/utils"
    },
    "modulePathIgnorePatterns": [
      "/bin",
      "/scripts",
      "/www"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.stencil/",
      "<rootDir>/bin/",
      "<rootDir>/dist/",
      "<rootDir>/mock-doc/",
      "<rootDir>/node_modules/",
      "<rootDir>/screenshot/",
      "<rootDir>/scripts/",
      "<rootDir>/server/",
      "<rootDir>/sys/",
      "<rootDir>/test/",
      "<rootDir>/testing/",
      "<rootDir>/www/"
    ],
    "testRegex": "/src/.*\\.spec\\.(ts|tsx|js)$"
  }
}
