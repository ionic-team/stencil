{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "declaration": false,
    "lib": ["dom", "es2015"],
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "sourceMap": false,
    "outDir": "../dist/scripts"
  }
}