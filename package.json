{
  "name": "@stencil/core",
  "version": "0.13.1",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "stencil": "bin/stencil"
  },
  "files": [
    "bin/",
    "compiler/",
    "dist/",
    "mock-doc/",
    "polyfills/",
    "screenshot/",
    "server/",
    "sys/",
    "testing/"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "build.cli": "node scripts/build-cli.js",
    "build.compiler": "node scripts/build-compiler.js",
    "build.core": "node scripts/build-core.js",
    "build.polyfills": "node scripts/build-polyfills.js",
    "build.renderer.vdom": "node scripts/build-renderer-vdom.js",
    "build.server": "node scripts/build-server.js",
    "build.dev.server": "node scripts/build-dev-server.js",
    "build.dev.server.client": "node scripts/build-dev-server-client.js",
    "build.mock-doc": "node scripts/build-mock-doc.js",
    "build.prod": "node scripts/build-prod.js",
    "build.screenshot": "node scripts/build-screenshot.js",
    "build.submodules": "node scripts/build-submodules.js",
    "build.sys.node": "node scripts/build-sys-node.js",
    "build.testing": "node scripts/build-testing.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "tslint --project src/tsconfig.json",
    "release": "node scripts/release.js",
    "release.prepare": "node scripts/release.js --prepare",
    "set.version": "node scripts/set-version.js",
    "start": "npm install && npm run build",
    "test": "jest",
    "test.dist": "node scripts/test-dist.js",
    "test.example": "cd test/example && npm test",
    "test.jest": "jest",
    "test.karma": "cd test/karma && npm run karma.prod",
    "test.prod": "npm run test.dist && npm run test.example && npm run test.jest && npm run test.karma && npm run test.server && npm run test.sys.node && npm run test.testing",
    "test.server": "cd test/server && npm run test.prod",
    "test.sys.node": "cd test/sys/node && npm run test.prod",
    "test.testing": "node scripts/test-testing.js",
    "test.watch": "jest --watch",
    "vermoji": "node scripts/vermoji.js"
  },
  "dependencies": {
    "chokidar": "2.0.4",
    "jsdom": "11.11.0",
    "rollup": "0.66.2",
    "rollup-plugin-commonjs": "9.1.8",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-pluginutils": "2.3.3",
    "typescript": "~2.9.1"
  },
  "devDependencies": {
    "@types/autoprefixer": "^6.7.3",
    "@types/chokidar": "^1.7.4",
    "@types/fs-extra": "^5.0.0",
    "@types/jest": "^23.3.1",
    "@types/mime-types": "^2.1.0",
    "@types/node": "9.4.5",
    "@types/parse5": "5.0.0",
    "@types/pixelmatch": "4.0.0",
    "@types/pngjs": "^3.3.2",
    "@types/puppeteer": "^1.6.0",
    "@types/ws": "6.0.1",
    "ansi-colors": "~3.0.6",
    "autoprefixer": "~9.1.5",
    "clean-css": "4.2.1",
    "conventional-changelog-cli": "^1.3.5",
    "css-what": "2.1.0",
    "estree-walker": "0.5.2",
    "execa": "1.0.0",
    "fast-deep-equal": "^2.0.1",
    "fs-extra": "^5.0.0",
    "glob": "7.1.3",
    "inquirer": "6.2.0",
    "is-glob": "4.0.0",
    "jest": "^23.5.0",
    "listr": "0.14.2",
    "magic-string": "0.25.1",
    "mime-db": "1.36.0",
    "minimatch": "3.0.4",
    "node-fetch": "1.7.3",
    "open-in-editor": "2.2.0",
    "opn": "5.4.0",
    "parse5": "5.1.0",
    "pixelmatch": "4.0.2",
    "postcss": "7.0.4",
    "puppeteer": "1.8.0",
    "semver": "5.5.1",
    "terser": "3.8.2",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "tslint-ionic-rules": "0.0.13",
    "webpack": "4.20.2",
    "ws": "6.0.0"
  },
  "lazyDependencies": {
    "@types/jest": "~23.3.1",
    "@types/puppeteer": "^1.6.0",
    "jest": "~23.5.0",
    "jest-environment-node": "~23.4.0",
    "pixelmatch": "4.0.2",
    "puppeteer": "~1.7.0",
    "workbox-build": "~3.4.1"
  },
  "engines": {
    "node": ">=6.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil.git"
  },
  "author": "Ionic Team",
  "homepage": "https://stenciljs.com/",
  "description": "A Compiler for Web Components and Progressive Web Apps",
  "keywords": [
    "web components",
    "components",
    "stencil",
    "ionic",
    "webapp",
    "custom elements",
    "pwa",
    "progressive web app"
  ],
  "jest": {
    "preset": "./testing/jest.preset.js",
    "modulePathIgnorePatterns": [
      "/bin",
      "/scripts",
      "/www"
    ],
    "moduleNameMapper": {
      "@stencil/core/mock-doc": "<rootDir>/mock-doc/"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/.stencil/",
      "<rootDir>/bin/",
      "<rootDir>/dist/",
      "<rootDir>/mock-doc/",
      "<rootDir>/node_modules/",
      "<rootDir>/screenshot/",
      "<rootDir>/scripts/",
      "<rootDir>/server/",
      "<rootDir>/sys/",
      "<rootDir>/test/",
      "<rootDir>/testing/",
      "<rootDir>/www/"
    ],
    "testRegex": "/src/.*\\.spec\\.(ts|tsx|js)$"
  }
}
