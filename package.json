{
  "name": "@stencil/core",
  "version": "1.12.4",
  "license": "MIT",
  "main": "internal/stencil-core.js",
  "types": "internal/stencil-core.d.ts",
  "bin": {
    "stencil": "bin/stencil"
  },
  "files": [
    "bin/",
    "cli/",
    "compiler/",
    "dev-server/",
    "internal/",
    "mock-doc/",
    "screenshot/",
    "sys/",
    "testing/"
  ],
  "scripts": {
    "start": "npm run watch",
    "build": "node scripts && npm run tsc.prod && npm run rollup.prod.ci",
    "build.prod": "node scripts && npm run tsc.scripts && npm run tsc.prod && npm run rollup.prod",
    "tsc.scripts": "tsc -p scripts/tsconfig.json",
    "watch": "node scripts && npm run tsc && concurrently \"npm run rollup.watch\" \"npm run tsc.watch\"",
    "release": "node scripts --release --publish",
    "release.prepare": "node scripts --release --prepare",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "tsc": "tsc --incremental",
    "tsc.prod": "tsc",
    "tsc.watch": "tsc --incremental --watch",
    "rollup": "rollup --config",
    "rollup.prod": "rollup --config --config-prod",
    "rollup.prod.ci": "rollup --config --config-prod --config-ci",
    "rollup.watch": "rollup --watch --config",
    "license": "node scripts --license",
    "prettier": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test.analysis": "cd test && npm run build",
    "test.dist": "node scripts --validate-build",
    "test.end-to-end": "cd test/end-to-end && npm i && npm test && npm run test.dist",
    "test.jest": "jest",
    "test.karma": "cd test/karma && npm run karma",
    "test.karma.prod": "cd test/karma && npm run karma.prod",
    "test.testing": "node scripts/test/validate-testing.js",
    "test.sys.node": "echo test.sys.node",
    "test.prod": "npm run test.dist && npm run test.end-to-end && npm run test.jest && npm run test.karma && npm run test.sys.node && npm run test.testing",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "11.1.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "7.1.3",
    "@rollup/plugin-replace": "^2.3.1",
    "@rollup/pluginutils": "^3.0.8",
    "@types/autoprefixer": "^9.7.2",
    "@types/exit": "^0.1.31",
    "@types/fs-extra": "^8.1.0",
    "@types/glob": "^7.1.1",
    "@types/graceful-fs": "^4.1.3",
    "@types/inquirer": "6.5.0",
    "@types/is-glob": "^4.0.1",
    "@types/jest": "^24.9.1",
    "@types/listr": "^0.14.2",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^13.11.1",
    "@types/node-fetch": "^2.5.5",
    "@types/parse5": "^5.0.2",
    "@types/pixelmatch": "^4.0.0",
    "@types/pngjs": "^3.4.2",
    "@types/prompts": "^2.0.5",
    "@types/puppeteer": "~2.0.1",
    "@types/semver": "^7.1.0",
    "@types/sizzle": "^2.3.2",
    "@types/webpack": "^4.41.10",
    "@types/ws": "^7.2.3",
    "ansi-colors": "4.1.1",
    "autoprefixer": "9.7.6",
    "concurrently": "^5.1.0",
    "conventional-changelog-cli": "^2.0.31",
    "core-js-builder": "~3.6.4",
    "css": "^2.2.4",
    "dts-bundle-generator": "~4.2.0",
    "execa": "4.0.0",
    "exit": "^0.1.2",
    "fast-deep-equal": "3.1.1",
    "fs-extra": "^9.0.0",
    "glob": "7.1.6",
    "graceful-fs": "~4.2.3",
    "hash.js": "^1.1.7",
    "inquirer": "^7.1.0",
    "is-glob": "4.0.1",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "jest-environment-node": "^24.9.0",
    "listr": "^0.14.3",
    "magic-string": "^0.25.7",
    "mime-db": "^1.43.0",
    "minimatch": "3.0.4",
    "node-fetch": "2.6.0",
    "open": "7.0.3",
    "open-in-editor": "2.2.0",
    "parse5": "5.1.1",
    "path-browserify": "^1.0.1",
    "pixelmatch": "4.0.2",
    "postcss": "^7.0.27",
    "prettier": "^2.0.4",
    "prompts": "2.3.2",
    "puppeteer": "~2.1.1",
    "rollup": "2.6.1",
    "semiver": "^1.1.0",
    "semver": "7.2.1",
    "sizzle": "^2.3.5",
    "stream-browserify": "^2.0.2",
    "terser": "4.6.11",
    "tslib": "^1.11.0",
    "util": "^0.12.2",
    "webpack": "^4.42.1",
    "ws": "7.2.3"
  },
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil.git"
  },
  "author": "Ionic Team",
  "homepage": "https://stenciljs.com/",
  "description": "A Compiler for Web Components and Progressive Web Apps",
  "keywords": [
    "web components",
    "components",
    "stencil",
    "ionic",
    "webapp",
    "custom elements",
    "pwa",
    "progressive web app"
  ]
}
