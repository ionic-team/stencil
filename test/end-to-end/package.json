{
  "name": "@stencil/end-to-end",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": " dist/collection/collection-manifest.json",
  "scripts": {
    "build": "node ../../bin/stencil build --next --docs",
