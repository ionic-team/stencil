{
  "compilerOptions": {
    "alwaysStrict": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "declaration": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es2017"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "../../dist/transpiled-build-conditionals",
    "pretty": true,
    "rootDir": "../",
    "target": "es5"
  },
  "include": [
    "../../types/*.d.ts"
  ],
  "files": [
    "./index.ts"
  ]
}
